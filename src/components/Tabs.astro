---
const routeName = [
    { name: "CASEROS", href: "/" },
    { name: "ONLYFANS", href: "/onlyfans" },
    { name: "TELEGRAM", href: "/telegram" },
    { name: "TIKTOK", href: "/tiktok" },
];
---

<div class="">
    <ul class="flex flex-nowrap -mb-px text-xs  font-medium text-center text-gray-500 dark:text-gray-400">
        {routeName.map((route) => (
            <li class="flex-1">
                <a href={route.href} class="w-full inline-flex items-center justify-center p-4 border-b-2 border-transparent rounded-t-lg hover:text-gray-600 hover:border-gray-300 dark:hover:text-gray-300 group">
                    <svg class="max-md:hidden w-4 h-4 me-2 text-gray-400 group-hover:text-gray-500 dark:text-gray-500 dark:group-hover:text-gray-300" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 20">
                        <path d="M10 0a10 10 0 1 0 10 10A10.011 10.011 0 0 0 10 0Zm0 5a3 3 0 1 1 0 6 3 3 0 0 1 0-6Zm0 13a8.949 8.949 0 0 1-4.951-1.488A3.987 3.987 0 0 1 9 13h2a3.987 3.987 0 0 1 3.951 3.512A8.949 8.949 0 0 1 10 18Z"/>
                    </svg>
                    {route.name}
                </a>
            </li>
        ))}
    </ul>
</div>
